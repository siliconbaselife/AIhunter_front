{
    "manifest_version": 3,
    "name": "AIhunter",
    "description": "AIhunter",
    "version": "1.0",
    "author": "lewis",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "*://*/*",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "scripting",
        "webRequest",
        "cookies"
    ]
}