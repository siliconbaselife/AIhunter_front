
            {
              "manifest_version": 3,
              "name": "AIhunter",
              "description": "AIhunter",
              "version": "1.0",
              "author": "lewis",
              "background": {
                  "service_worker": "background.js",
                  "type": "module"
              },
              "content_scripts": [
                {
                    "matches": [
                        "https://*.maimai.cn/*",
                        "https://*.linkedin.com/*"
                    ],
                    "js": [
                        "content.js"
                    ],
                    "run_at": "document_start"
                },
                {
                    "matches": [
                        "https://*.baidu.com/*"
                    ],
                    "js": [
                        "content.js"
                    ],
                    "run_at": "document_start"
                }
              ],
              "host_permissions": [
                  "*://*/*",
                  "http://*/*",
                  "https://*/*",
                  "<all_urls>"
              ],
              "permissions": [
                  "tabs",
                  "storage",
                  "activeTab",
                  "scripting",
                  "webRequest",
                  "cookies"
              ]
            }
        